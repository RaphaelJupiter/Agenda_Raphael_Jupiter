<Window x:Class="Agenda_Raphael_Jupiter.NewMemberWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Add New Member" Height="300" Width="400">
    <StackPanel Margin="10">
        <TextBlock Text="Name:" />
        <TextBox x:Name="NameTextBox" />

        <TextBlock Text="Prenom:" Margin="0,10,0,0"/>
        <TextBox x:Name="PrenomTextBox" />

        <TextBlock Text="Email:" Margin="0,10,0,0"/>
        <TextBox x:Name="EmailTextBox" />

        <TextBlock Text="Phone:" Margin="0,10,0,0"/>
        <TextBox x:Name="PhoneTextBox" />

        <Button Content="Add" Click="AddButton_Click" Margin="0,20,0,0"/>
    </StackPanel>
</Window>